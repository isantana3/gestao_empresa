<%- include("head.ejs"); %>
<body>
    <div class="table-responsive-sm">
        <table class="table table-striped table-sm">
            <thead>
                <tr>
                    <th>Cliente</th>
                    <th>Nota Fiscal</th>
                    <th>Setor</th>
                    <th>Valor</th>
                    <th>Data</th>
                    <th class="d-print-none">
                    </th>
                </tr>
            </thead>
            <tbody>
                <% for (const item of dados) { %>
                    <tr>
                        <td>
                            <%= item.cliente %>
                        </td>
                        <td>
                            <%= item.nota_fiscal %>
                        </td>
                        <td>
                            <%= item.setor_vendas %>
                        </td>
                        <td>
                            <%= item.valor_vendas %>
                        </td>
                        <td>
                            <%= item.data_venda %>
                        </td>
                        <td class="d-print-none">
                        </td>
                    </tr>
                    <%} %>
            </tbody>
        </table>
    </div>
</body>

</html>